<div class="container">
  <div class="row">
    <div class="col">
      <h1>Book this brain</h1>
      <%= simple_form_for [@brain, @booking]  do |f| %>
      <%= f.input :start_date,
                  as: :string,
                  input_html: { data: { controller: "datepicker", datepicker_date_value: @dates_booking}} %>
      <%= f.input :end_date,
                  as: :string,
                  input_html: { data: { controller: "datepicker", datepicker_date_value: @dates_booking}} %>
      <%= f.submit %>
    <% end %>
    </div>
  </div>
</div>
