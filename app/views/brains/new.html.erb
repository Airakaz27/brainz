<%= simple_form_for(@brain) do |f| %>
  <%= f.input :name %>
  <%= f.input :qi %>
  <%= f.input :languages, collection: Brain::LANGUAGES, multiple: true, as: :check_boxes, include_hidden: false %>
  <%= f.input :specialties, collection: Brain::SPECIALTIES, multiple: true, as: :check_boxes, include_hidden: false %>
  <%= f.input :price_per_day %>
  <%= f.input :photo, as: :file %>
  <%= f.submit %>
<% end %>
